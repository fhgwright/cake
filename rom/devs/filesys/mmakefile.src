# $Id$
include $(TOP)/config/make.cfg

OS_INCLUDES	:= -I$(GENINCDIR)
OBJDIR		:= $(GENDIR)/filesys

#MM
kernel-filesys-$(ARCH)-$(CPU) : show-flags $(OBJDIR)/emul_handler.o

show-flags :
	@$(ECHO) "CFLAGS=$(CFLAGS)"

#MM kernel-filesys : setup includes kernel-filesys-$(ARCH)-$(CPU)
kernel-filesys :
	@$(NOP)

#MM
setup :
	%mkdirs_q $(OBJDIR)

#MM
clean ::
	-$(RM) $(OBJDIR)

$(OBJDIR)/%.o : %.c
	%compile_q

$(OBJDIR)/%.d : %.c
	%mkdepend_q

%common
%include_deps $(OBJDIR)/emul_handler.d
