    This file is generated from $filename.
*/
<IF cond="$first">
<SET first=0>
#include "$basename($filename).h"
</IF>

<if cond="$defined(local_includes)">
<WRITE FILE="$basename($filename).h" TEXT="$local_includes">
</if>

<if cond="$defined(includes)">
<WRITE FILE="$basename($filename).h" TEXT="$includes">
</if>

AROS_LH$numargs${funcflags}
($returntype, ${funcname},
<if cond="$defined(synopsis)">
    $synopsis,
</if>
    $libbasetype, $libbasename, $offset, $libname
)
{
AROS_LIBFUNC_INIT
<if cond="$defined(source)">
<code type=c>
$source
</code>
</if>
AROS_LIBFUNC_EXIT
}
