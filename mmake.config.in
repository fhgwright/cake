[AROS]
# You need only uncomment the next line if you run mmake from somewhere
# else than $(TOP).
#top /home/digulla/AROS
maketool make TOP="$(TOP)" CURDIR="$(CURDIR)" TARGET=$(TARGET)
defaultmakefilename mmakefile
defaulttarget AROS
# Uncomment this if you want/have to use gawk
#genmakefilescript gawk -f $(TOP)/scripts/genmf.gawk --assign "TOP=$(TOP)" < mmakefile.src > mmakefile
genmakefilescript $(TOP)/local/genmf/genmf $(TOP)/config/make.tmpl < mmakefile.src > mmakefile
genmakefiledeps $(TOP)/scripts/genmf.gawk $(TOP)/config/make.tmpl
globalvarfile $(TOP)/config/host.cfg
genglobalvarfile sh $(TOP)/configure -m $(TOP)/config/host.cfg

ignoredir CVS
ignoredir bin
