# $Id$
include $(TOP)/config/make.cfg

USER_INCLUDES	:= -I/usr/include
OBJDIR		:= $(GENDIR)/filesys

#MM kernel : setup includes
kernel : show-flags $(OBJDIR)/emul_handler.o

show-flags :
	@$(ECHO) "CFLAGS=$(CFLAGS)"

#MM
setup :
	%mkdirs_q $(OBJDIR)

#MM
clean ::
	-$(RM) $(OBJDIR)

$(OBJDIR)/%.o : %.c
	%compile_q

$(OBJDIR)/%.d : %.c
	%mkdepend_q

%common
%include_deps $(OBJDIR)/emul_handler.d
