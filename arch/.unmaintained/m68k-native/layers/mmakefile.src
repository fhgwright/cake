# $Id$
TOP=../../..

include $(TOP)/config/make.cfg

SPECIAL_AFLAGS = -I../
CLIBDIR=$(GENDIR)/clib
OBJDIR=$(OSGENDIR)/layers

FILES = basicasmfuncs

#MM
setup-layers-amiga-m68k :
	%mkdirs_q $(OBJDIR) $(OSGENDIR)
	%add_objects $(foreach f,$(FILES), rom/layers/$(f))

#MM
clean::
	$(RM) $(TESTS)

$(OBJDIR)/%.o: %.s ../machine.i
	%assemble_q

%common
