MMAKE=@MMAKE@

ifeq ($(MMAKE),)
MMAKE=./mmake
all: mmake genmf archtool
else
all: genmf archtool
endif
	$(MMAKE)

mmake:
	cd tools/MetaMake; ./configure
	$(MAKE) -C tools/MetaMake
	cp tools/MetaMake/mmake .

genmf:
	$(MAKE) -C tools/genmf

archtool:
	$(MAKE) -C tools/archtools

clean:
	$(MMAKE) AROS.clean

