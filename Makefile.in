MMAKE=@MMAKE@

ifeq ($(MMAKE),)
MMAKE=./mmake
all: mmake genmf
else
all: genmf
endif
	$(MMAKE)

mmake:
	cd tools/MetaMake; ./configure
	$(MAKE) -C tools/MetaMake
	cp tools/MetaMake/mmake .

genmf:
	$(MAKE) -C tools/genmf

clean:
	$(MMAKE) AROS.clean

