<OUTPUT FILE="$funcname.c">
<WRITE FILE="file.list" TEXT="$funcname.c">
/*
    (C) 1995-97 AROS - The Amiga Replacement OS

    This file is generated from $filename.
*/

<if cond="$defined(local_includes)">
<ecode type=c>
$local_includes
</ecode>
</if>

<if cond="$defined(includes)">
<ecode type=c>
$includes
</ecode>
</if>

AROS_LH$numargs${funcflags}
($returntype, ${funcname},
<if cond="$defined(synopsis)">
    $synopsis,
</if>
    $libbasetype, $libbasename, $offset, $libname
)
{
AROS_LIBFUNC_INIT
<if cond="$defined(source)">
<code type=c>
$source
</code>
</if>
AROS_LIBFUNC_EXIT
}
